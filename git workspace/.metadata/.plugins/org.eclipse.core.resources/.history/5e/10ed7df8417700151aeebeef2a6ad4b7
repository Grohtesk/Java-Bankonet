package command;

import bankonet.metier.ClientService;
import bankonet.metier.client.InitService;

public class InitCommand extends IhmCommand {

	ClientService clientService;
	
	public InitCommand(ClientService clientService) {
		this.clientService=clientService;
	}

	@Override
	public int getInt() {
		// TODO Auto-generated method stub
		return 3;
	}

	@Override
	public String getLibelle() {
		return "Intialise des clients avec JPA";
	}

	@Override
	public void execute() {
		InitService is=new InitService(null);
		is.init();
	}

	

}
